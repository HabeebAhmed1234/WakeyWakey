<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tableLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_marginTop="20sp" >
    
<TableRow
        android:id="@+id/tableRow1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dip" >
        
    <LinearLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/Alarm"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#B0C4DE"
            android:padding="5sp"
            android:layout_marginLeft="10sp"
            android:layout_marginRight="10sp" >
            
			<TextView
                android:id="@+id/time"
                android:layout_width="fill_parent"
            	android:textSize="30sp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:text="1:00 AM" />
			
			<LinearLayout
			    android:orientation="horizontal"
			    android:layout_width="fill_parent"
                android:layout_height="wrap_content"
			    >
	            <ImageView
	                android:id="@+id/facebook"
	                android:src="@drawable/facebook_icon"
	                android:layout_width="30dp"
	                android:layout_height="30dp"
	                android:layout_margin="1sp"/>
	
	            <ImageView
	                android:id="@+id/music"
	                android:layout_width="30dp"
	                android:layout_height="30dp"
	                android:src="@drawable/text_contacts"
	                android:layout_margin="1sp"/>
	
	            <ImageView
	                android:id="@+id/text_contacts"
	                android:layout_width="30dp"
	                android:layout_height="30dp"
	                android:src="@drawable/music_icon"
	                android:layout_margin="1sp"/>
			</LinearLayout>
	</LinearLayout>
	
	<RelativeLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/Alarm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#B0C4DE"
            android:padding="5sp"
            android:layout_marginLeft="10sp"
            android:layout_marginRight="10sp" >

            <ImageView
                android:id="@+id/facebook"
                android:src="@drawable/facebook_icon"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignTop="@+id/music"
                android:layout_toRightOf="@+id/music" 
                android:layout_margin="1sp"/>

            <ImageView
                android:id="@+id/music"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/text_contacts"
                android:layout_alignTop="@+id/text_contacts"
                android:layout_toRightOf="@+id/text_contacts" 
                android:layout_margin="1sp"/>

            <ImageView
                android:id="@+id/text_contacts"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/music_icon"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/time" 
                android:layout_margin="1sp"/>

            <TextView
                android:id="@+id/time"
                android:layout_width="fill_parent"
            	android:textSize="30sp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:text="1:00 AM" />
        </RelativeLayout>
    </TableRow>
